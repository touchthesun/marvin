{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "lib": ["dom", "es6", "es2017"],
      "outDir": "./dist",
      "rootDir": "./src",
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }